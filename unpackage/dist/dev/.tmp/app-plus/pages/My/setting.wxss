page {
  background: #f3f3f3;
}
.setting {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  background: #FFFFFF;
}
.setting .setting-item {
  width: 720rpx;
  margin-left: 30rpx;
  height: 100rpx;
  background: #FFFFFF;
  border-bottom: 2rpx solid #F4F4F5;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 14px;
  color: #222222;
}
.setting .setting-item > view:first-child {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.setting .setting-item > view:last-child {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
.setting .setting-item > view:last-child image {
  width: 16rpx;
  height: 26rpx;
  margin-right: 30rpx;
}
.setting .setting-item > view:last-child text {
  font-size: 14px;
  color: #A2A2A2;
  margin-right: 30rpx;
}
.loginOut {
  width: 100%;
  height: 100rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  font-size: 14px;
  color: #C9281C;
  background: #FFFFFF;
}

