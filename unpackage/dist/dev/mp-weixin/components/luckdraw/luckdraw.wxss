/**小圆点 start*/
.tui-dot {
	position: absolute;
	display: block;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	height: 20rpx;
	width: 20rpx;
}
.tui-dot:nth-child(odd) {
	background: #fff;
	-webkit-animation: 0.5s odd linear infinite;
	        animation: 0.5s odd linear infinite;
}
.tui-dot:nth-child(even) {
	background: #fcf400;
	-webkit-animation: 0.5s even linear infinite;
	        animation: 0.5s even linear infinite;
}
